# set aliases for different programming language environments
alias cenv="alias e='vim main.c'; alias a='make all'; alias r='./a.out'"
alias cppenv="alias e='vim main.cpp'; alias a='make all'; alias r='./a.out'"
alias pyenv="alias e='vim main.py'; alias r='python3 main.py'"

# silence gdb version information on startup
alias gdb="gdb -q"
